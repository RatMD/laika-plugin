{
    "name": "ratmd/laika-plugin",
    "type": "october-plugin",
    "version": "0.1.0",
    "description": "LAIKA is an Inertia-inspired Vue/Vite adapter which lets you build your entire OctoberCMS theme in Vue while still using everything October provides.",
    "keywords": [
        "october",
        "octobercms",
        "bridge",
        "vue"
    ],
    "homepage": "https://octobercms.com/plugin/ratmd-laika",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "sam@rat.md",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@rat.md",
        "issues": "https://www.github.com/RatMD/laika-plugin/issues",
        "source": "https://www.github.com/RatMD/laika-plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 8.2",
        "composer/installers": "~2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
